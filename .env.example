DATABASE_URL=postgresql://postgres:xyz@localhost:5432/icon_backend?schema=public
PORT=8080
CORS_ORIGINS=*

# Auth configuration
# clerk (default) or native
AUTH_MODE=clerk
# Clerk JWKS endpoint (e.g., https://clerk.example.com/.well-known/jwks.json)
CLERK_JWKS_URL=
# One-time secret to allow bootstrap of first admin
ADMIN_BOOTSTRAP_SECRET=
 # Native JWT signing secret (used when AUTH_MODE=native)
 JWT_SECRET=

# S3 configuration (common)
S3_REGION=us-east-1
S3_BUCKET=your-bucket-name
S3_PUBLIC_BASE=https://your-bucket-public-base
S3_ACCESS_KEY_ID=
S3_SECRET_ACCESS_KEY=